{
  "name": "thread",
  "description": "Lichine's personal toolkit for creating progressive web stuff.",
  "version": "1.0.0",
  "author": "Anders Schmidt Hansen <me@andersschmidt.com>",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run set-development\" \"screwy\"",
    "set-development": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "deploy": "concurrently \"npm run build\" \"firebase deploy\""
  },
  "dependencies": {
    "vue": "^2.1.0",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.1.1",
    "vuex": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.18.0",
    "concurrently": "^3.1.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "screwy": "^1.0.5",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  }
}
